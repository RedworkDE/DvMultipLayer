@model IEnumerable<RedworkDE.DVMP.Server.Data.UserInfo>

@{
    ViewData["Title"] = "UserList";
}

<h1>UserList</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>

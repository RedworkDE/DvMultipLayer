            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HasIpV4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HasIpV6)
